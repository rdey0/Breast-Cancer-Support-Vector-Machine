    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "python": "0.0.4",
    "python-shell": "^2.0.2",
    "python3": "0.0.1",
    "react-scripts": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "gh-pages": "^3.1.0"
  }
}
